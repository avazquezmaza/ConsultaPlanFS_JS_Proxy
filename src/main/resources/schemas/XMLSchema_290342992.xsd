			<xsd:element name="interactionStatusBusinessInteraction" type="p1:interactionStatusBusinessInteractionType" minOccurs="1" maxOccurs="1" nillable="false"/>
			<xsd:element name="statusDetailInfo" type="p1:partyRoleIdPartyRoleType" minOccurs="0" maxOccurs="1" nillable="false"/>
			<xsd:element name="customerStatusTime" type="p1:startDateTimeTimePeriodType" minOccurs="0" maxOccurs="1" nillable="false"/>
			<xsd:element name="relaPrimaryOfferingInstanceIdInfo" type="p1:objectIDRootEntityType" minOccurs="0" maxOccurs="1" nillable="false"/>
			<xsd:element name="parentIdInfo" type="p1:objectIDRootEntityType" minOccurs="0" maxOccurs="1" nillable="false"/>
			<xsd:element name="startDateTimeTimePeriod" type="p1:startDateTimeTimePeriodType" minOccurs="0" maxOccurs="1" nillable="false"/>
			<xsd:element name="endDateTimeTimePeriod" type="p1:endDateTimeTimePeriodType" minOccurs="0" maxOccurs="1" nillable="false"/>
